{
  "services": [
    {
      "name": "qbt",
      "image": "ghcr.io/hotio/qbittorrent:release-5.0.5",
      "container_name": "qbt",
      "dns": ["1.1.1.1"],
      "cap_add": ["NET_ADMIN"],
      "ports": [
        "8089:8089",
        "8118:8118"
      ],
      "sysctls": {
        "net.ipv4.conf.all.src_valid_mark": 1,
        "net.ipv6.conf.all.disable_ipv6": 1
      },
      "volumes": [
        {
          "hostPath": "~/docker/qbt",
          "containerPath": "/config"
        },
        {
          "hostPath": "/mnt",
          "containerPath": "/mnt"
        },
        {
          "hostPath": "~/docker/themeparkdev/98-themepark-qbittorrent",
          "containerPath": "/etc/cont-init.d/98-themepark"
        }
      ]
    }
  ]
}